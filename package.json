{
  "name": "appwork-vue-starter",
  "version": "1.3.1",
  "private": true,
  "description": "## Project setup ``` yarn install ```",
  "author": "",
  "scripts": {
    "serve-local": "vue-cli-service serve --mode local",
    "build-local": "vue-cli-service build --mode local",
    "serve-dev": "vue-cli-service serve --mode develoment",
    "build-dev": "vue-cli-service build --mode develoment",
    "serve-stg": "vue-cli-service serve --mode staging",
    "build-stg": "vue-cli-service build --mode staging",
    "serve-prd": "vue-cli-service serve --mode production",
    "build-prd": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "main": "babel.config.js",
  "dependencies": {
    "@toast-ui/vue-grid": "^4.10.0",
    "axios": "~0.18.0",
    "bootstrap": "~4.3.1",
    "bootstrap-vue": "2.0.0-rc.16",
    "core-js": "^2.6.11",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.0",
    "perfect-scrollbar": "~1.4.0",
    "quill": "^1.3.7",
    "quill-image-drop-module": "^1.0.3",
    "spinkit": "~1.2.5",
    "vee-validate": "^2.2.10",
    "vue": "~2.6.6",
    "vue-flatpickr-component": "~8.1.2",
    "vue-meta": "~1.6.0",
    "vue-quill": "^1.5.1",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "~3.0.1",
    "vuex": "^3.1.2",
    "vuetify": "1.1.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "html-webpack-exclude-assets-plugin": "^0.0.7",
    "node-sass": "~4.13.1",
    "rename-webpack-plugin": "^2.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "suppress-chunks-webpack-plugin": "^1.0.5",
    "vue-template-compiler": "~2.6.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": false
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    ">= 1%",
    "last 2 versions",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "eslintIgnore": [
    "/dist/",
    "/*.js",
    "/tests/unit/coverage/",
    "src/vendor/libs/**/*.js"
  ],
  "license": "ISC",
  "resolutions": {
    "sass-loader/node-sass": "~4.13.1"
  }
}
